{
  "manifest_version": 2,
  "name": "Miles' extention",
  "description": "Browser extention",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file://*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "images/3d-pixel-ghost-orange-128x128.png"
  }
}
